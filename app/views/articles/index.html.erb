


<h1>Articles</h1>

  <% @articles.each do |article| %>
    <% unless article.archived? %>
      <div class="card" style="width: 18rem; margin-bottom:1rem;">
  <div class="card-body">
    <h5 class="card-title"><%= article.title %> </h5>
    <%= link_to article.title, article %> 
    
  </div>
</div>
    <% end %>
  <% end %>


